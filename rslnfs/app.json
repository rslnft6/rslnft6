{
  "expo": {
    "name": "BaitVR",
    "slug": "baitvr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/favicon.ico",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/favicon.ico",
      "resizeMode": "contain",
      "backgroundColor": "#e0eafc"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baitvr.app"
    },
    "android": {
      "package": "com.baitvr.app"
    },
    "web": {
      "favicon": "./public/favicon.ico"
    }
  }
}
